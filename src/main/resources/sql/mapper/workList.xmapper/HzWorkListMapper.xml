        )values(
        #{changeNum,jdbcType=VARCHAR},#{launcher,jdbcType=VARCHAR},
        #{launcherId,jdbcType=VARCHAR},#{launcherDep,jdbcType=VARCHAR},
        #{launcherContact,jdbcType=VARCHAR},#{auditer,jdbcType=VARCHAR},
