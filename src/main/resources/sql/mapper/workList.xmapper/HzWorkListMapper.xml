	<!--统计待办事项-->
	<select id="HzWorkListDAOImpl_count" parameterType="java.util.Map" resultType="int">
		SELECT count(*) FROM HZ_WORK_LIST
		WHERE 1=1
		<!--接口（审批）人是自己（登录角色）-->
		and AUDITER = #{auditer,jdbcType=VARCHAR}
		<if test="projectId != null and projectId !='' ">
